 <div class='col-md-5 col-md-offset-3'>
  <% @trackers.each do |tracker| %>
    <table class='table table-hover table-bordered'>
      <tbody>
        <thead>
          <th> Date: </th>
          <th>Day Total:</th>
          <th>Overall Points:</th>       
        </thead>
        <tr>
          <td><%= tracker.day %></td>
          <td><%= tracker.day_points %></td>
          <td><%= tracker.overall_points %></td>
        </tr>
      </tbody>
      <% end %>
    </table>
  </div>


